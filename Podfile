# Uncomment this line to define a global platform for your project
platform :ios, '9.0'
use_frameworks!
target 'Demo' do
    # autolayout
    pod 'Masonry'
    # 测试工具栏，只在debug下面有效
    pod 'FLEX', :configurations => ['Debug']
    pod 'AFNetworking'
    pod 'YYModel'
    pod 'MJRefresh'
    pod 'SDWebImage'
    pod 'MBProgressHUD'
    # 生成device uuid库
    pod 'FCUUID'
    # 文字跑马灯效果
    pod 'MarqueeLabel'
    pod 'YYCache'
    pod 'MMKV', '~> 1.2.13'
end

